{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~": ["./app"],
      "~/*": ["./app/*"],
      "@": ["./app"],
      "@/*": ["./app/*"],
      "~~": ["."],
      "~~/*": ["./*"],
      "@@": ["."],
      "@@/*": ["./*"],
      "#types": ["./types"],
      "#types/*": ["./types/*"]
    }
  }
}
